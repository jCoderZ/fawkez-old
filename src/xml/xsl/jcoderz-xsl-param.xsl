<?xml version='1.0'?>
<xsl:stylesheet version='1.0'
                xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

<!-- parameters for docbook templates -->
  <xsl:param name="admon.graphics">0</xsl:param>
  <xsl:param name ="alignment">left</xsl:param>
  <xsl:param name ="body.font.family">sans-serif</xsl:param>
  <xsl:param name ="body.font.master">10</xsl:param>
  <xsl:param name ="body.margin.top">1.5cm</xsl:param>
  <xsl:param name ="body.start.indent">2pt</xsl:param>
  <xsl:param name ="chapter.autolabel">1</xsl:param>
  <xsl:param name ="column.count.back">1</xsl:param>
  <xsl:param name ="column.count.body">1</xsl:param>
  <xsl:param name ="column.count.front">1</xsl:param>
  <xsl:param name ="column.count.index">1</xsl:param>
  <xsl:param name ="css.decoration">1</xsl:param>
  <xsl:param name ="double.sided">0</xsl:param>
  <xsl:param name ="draft.mode">no</xsl:param>
  <xsl:param name ="draft.watermark.image"></xsl:param>
  <xsl:param name ="fop.extensions">1</xsl:param>
  <xsl:param name ="generate.index">1</xsl:param>
  <xsl:param name ="generate.toc">book toc,title,figure</xsl:param>
  <xsl:param name ="insert.xref.page.number">0</xsl:param>
  <xsl:param name ="make.valid.html">1</xsl:param>
  <xsl:param name ="page.orientation">portrait</xsl:param>
  <xsl:param name ="paper.type">A4</xsl:param>
  <xsl:param name ="section.autolabel">1</xsl:param>
  <xsl:param name ="section.autolabel.max.depth">3</xsl:param>
  <xsl:param name ="section.label.includes.component.label">1</xsl:param>
  <xsl:param name ="region.before.extent">1.2cm</xsl:param>
<!--  <xsl:param name ="shade.verbatim">1</xsl:param> -->
  <xsl:param name ="table.borders.with.css">1</xsl:param>
  <xsl:param name ="tablecolumns.extension">0</xsl:param>
  <xsl:param name ="title.margin.left">-4pc</xsl:param>
  <xsl:param name ="toc.section.depth">2</xsl:param>
  <xsl:param name ="use.extensions">1</xsl:param>
  <xsl:param name ="ulink.show">0</xsl:param>
  <xsl:param name ="ulink.footnotes">1</xsl:param>

<xsl:param name="formal.title.placement">
figure after
example before
equation after
table before
procedure before
</xsl:param>

</xsl:stylesheet>
